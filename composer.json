{
    "name": "msvujnovic/php-fqcn-parser",
    "description": "Extracts a list of fully qualified class names from a list of files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko S. Vujnovic",
            "email": "msvujnovic@gmail.com"
        }
    ],
    "require": {
        "tightenco/collect": "^5.3",
        "symfony/console": "^3.4",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "codacy/coverage": "^1.4",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "autoload": {
        "psr-4": {
            "PhpFqcnParser\\": "src/"
        }
    }
}
